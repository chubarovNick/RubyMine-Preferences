<?xml version="1.0" encoding="UTF-8"?>
<keymap version="1" name="Awesome keymap" parent="Default for Mac OS X">
  <action id="Annotate">
    <keyboard-shortcut first-keystroke="control G" second-keystroke="control B" />
  </action>
  <action id="ChangeSplitOrientation">
    <keyboard-shortcut first-keystroke="alt C" />
  </action>
  <action id="Git.Branches">
    <keyboard-shortcut first-keystroke="control G" second-keystroke="B" />
  </action>
  <action id="Git.Pull">
    <keyboard-shortcut first-keystroke="control G" second-keystroke="P" />
  </action>
  <action id="Git.Push">
    <keyboard-shortcut first-keystroke="control G" second-keystroke="control P" />
  </action>
  <action id="Git.Rebase">
    <keyboard-shortcut first-keystroke="control G" second-keystroke="control R" />
  </action>
  <action id="Git.Rebase.Continue">
    <keyboard-shortcut first-keystroke="control G" second-keystroke="control C" />
  </action>
  <action id="Git.ResolveConflicts">
    <keyboard-shortcut first-keystroke="control G" second-keystroke="R" />
  </action>
  <action id="Git.Stash">
    <keyboard-shortcut first-keystroke="control G" second-keystroke="control S" />
  </action>
  <action id="Git.Unstash">
    <keyboard-shortcut first-keystroke="control G" second-keystroke="control U" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="alt S" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="control alt ENTER" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="control alt DOWN" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="control alt LEFT" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="control alt RIGHT" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="control alt UP" />
  </action>
  <action id="Unsplit">
    <keyboard-shortcut first-keystroke="meta alt S" />
  </action>
  <action id="Vcs.ShowTabbedFileHistory">
    <keyboard-shortcut first-keystroke="control G" second-keystroke="control H" />
  </action>
</keymap>

